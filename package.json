{
  "name": "electron-quick-start-typescript",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "tsc:build": "tsc",
    "tsc:watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "electron:run": "electron  --disable-gpu ./dist/main2.js ",
    "start-electron": "npm run build && electron ./dist/main2.js",
    "start-all": "npm-run-all -s tsc:build electron:run",
    "start": "npm run electron:run"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/jquery": "^3.3.1",
    "@types/lodash": "^4.14.108",
    "electron": "^1.8.6",
    "electron-reload": "^1.2.2",
    "electron-reloader": "^0.2.0",
    "npm-run-all": "^4.1.3",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "colors": "^1.2.4",
    "dotenv": "^5.0.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10"
  }
}
